<Application x:Class="MyFirstMVVM.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MyFirstMVVM"
             xmlns:conv="clr-namespace:MyFirstMVVM.Converters"
             StartupUri="View/ListExplanation.xaml">
    <Application.Resources>
        <Style x:Key="LabelStandard" TargetType="Label">
            <Setter Property="FontSize" Value="32"/>
        </Style>

        <conv:EnumSexToStringValueConverter x:Key="EnumToSexToString"/>
        <conv:VisibleToBoolValueConverter x:Key="VisibleToBool"/>
    </Application.Resources>
</Application>